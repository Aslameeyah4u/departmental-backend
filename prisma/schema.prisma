// This is your Prisma schema file,
// learn more about it in the docs https//pris.ly/d/prisma-schema

generator joi {
  provider = "prisma-joi-generator"
  output   = "./joi"
}

generator erd {
  provider = "prisma-erd-generator"
  output   = "./erd/erd.svg"
}

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

